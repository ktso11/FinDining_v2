
  <div class="container">
    <div class="row">
      <div class="col-md-12">
        <h3>Log In</h3>
        <br>
        <form method="POST" action="/partials/login">
          <div class="form-group">
            <input type="text" name="username" class="form-control" placeholder="Username" autofocus>
          </div>
          <div class="form-group">
            <input type="password" name="password" class="form-control" placeholder="Password">
          </div>
          <div class="form-group">
            <input type="submit" class="login-btn btn btn-block btn-primary" value="Log In">
          </div>
        </form>
      </div>
    </div>
  </div>
